<%}%>

    public <%= n %>Dto(
<%= ctxt.getColumnDeclarations() %>    
    )
    {
        super();
<% for (final Column col : ctxt.getColumns()) { %>
        this.<%= col.getNameAsProperty() %> = <%= col.getNameAsProperty() %>; 
<%}%>
    }

<% for (final Column col : ctxt.getColumns()) { %>
    /**
     * <%= col.getNameAsProperty() %>
     */
